#include "CudaEcbAes16B.cuh"

__global__ void cuda_ecb_aes_16b_encrypt(unsigned char data[], int n_blocks, AES_KEY* key)
{


}


__global__ void cuda_ecb_aes_16b_decrypt(unsigned char data[], int n_blocks, AES_KEY* key)
{


}

void cuda_ecb_aes_16b_encrypt(unsigned char data[], int n_blocks, AES_KEY* key, int gridSize, int threadsPerBlock)
{
	cuda_ecb_aes_16b_encrypt<<gridSize,threadsPerBlock>>(data,n_blocks,key);
}


void cuda_ecb_aes_16b_decrypt(unsigned char data[], int n_blocks, AES_KEY* key, int gridSize, int threadsPerBlock)
{
	cuda_ecb_aes_16b_decrypt<<gridSize,threadsPerBlock>>(data,n_blocks,key);
}
