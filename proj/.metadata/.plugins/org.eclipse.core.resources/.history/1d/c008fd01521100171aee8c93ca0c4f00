#include "CudaEcbAes16B.cuh"

__global__ void cuda_ecb_aes_16b_encrypt_kernel(unsigned char data[], int n_blocks, AES_KEY* key)
{


}


__global__ void cuda_ecb_aes_16b_decrypt_kernel(unsigned char data[], int n_blocks, AES_KEY* key)
{


}

void cuda_ecb_aes_16b_encrypt(int gridSize, int threadsPerBlock, unsigned char data[], int n_blocks, AES_KEY* key)
{
	cuda_ecb_aes_16b_encrypt_kernel<<gridSize,threadsPerBlock>>(data,n_blocks,key);
}


void cuda_ecb_aes_16b_decrypt(int gridSize, int threadsPerBlock, unsigned char data[], int n_blocks, AES_KEY* key)
{
	cuda_ecb_aes_16b_decrypt_kernel<<gridSize,threadsPerBlock>>(data,n_blocks,key);
}
